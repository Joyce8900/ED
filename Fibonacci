#include <stdio.h>
int c = 0;


int fibR (int n ) {
  c++;
    if (n<2)
        return n;
    
    return fibR (n-1)+ fibR (n-2);
    
}
unsigned long int fibrI (int n ) {
    if (n<2)
        return n;
    
    int n1 = 1;
    int n2 = 0;
    int v ;
  c++;
    for (int i = 0;i<n;i=i+1){
        v = n1;
        n1 = n1+n2;
        n2=v;
        c++;
    }
    return v;
}

int main (void){
    printf("%lu\n",fibrI(30));
    //printf("%d\n",fibR(30));
    printf("%d\n",c);
}
